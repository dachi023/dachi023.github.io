  "scripts": {
    "build": "gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "copy:cname": "cp CNAME ./public/CNAME",
    "deploy": "run-s clean build copy:cname publish",
    "format": "eslint ./src --ext .ts,.tsx --fix",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./src --ext .ts,.tsx",
    "lint:ts": "tsc --noEmit",
    "publish": "gh-pages -d ./public",
    "serve": "gatsby serve",
    "start": "gatsby develop"
  },
